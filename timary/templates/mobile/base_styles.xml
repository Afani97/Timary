<style id="Body" backgroundColor="#1b1d23" flex="1" />
<style id="Main" backgroundColor="#2b303b" flex="1" paddingTop="12" />
<style id="Wrapper"
    display="flex"
    justifyContent="center"
    flexDirection="row"
    flexWrap="wrap"
    width="100%"
/>
<style
    id="FormGroup"
    flex="1"
    marginLeft="15"
    marginRight="15"
    marginTop="15"
/>
<style
    id="Input"
    flex="1"
    fontSize="18"
    fontWeight="normal"
    padding="12"
    color="#9da1ad"
    borderColor="black"
    borderWidth="1"
    borderRadius="10"
    backgroundColor="#1b1d23"
    overflow="hidden"
>
    <modifier focused="true">
      <style borderColor="#5d15dd" />
    </modifier>
</style>
<style
    id="Input--Error" marginLeft="20" marginRight="20" borderBottomColor="#FF4847" color="#FF4847"
>
    <modifier focused="true">
      <style borderBottomColor="#FF4847" />
    </modifier>
</style>
<style
    id="DatePickerModal"
    backgroundColor="#1b1d23"
    color="white"
/>
<style
    id="DatePickerModal__text"
    backgroundColor="#1b1d23"
    color="white"
    fontSize="20"
    fontWeight="600"
    padding="24"
    paddingBottom="0"
>
</style>
<style
    id="PickerModal"
    backgroundColor="white"
    color="#1b1d23"
/>
<style
    id="PickerModal__text"
    backgroundColor="white"
    color="#1b1d23"
    fontSize="20"
    fontWeight="600"
    padding="24"
    paddingBottom="0"
>
</style>
<style
    id="Label"
    color="#9da1ad"
    fontSize="16"
    fontWeight="bold"
    lineHeight="24"
    marginBottom="8"
/>
<style
    id="Help"
    borderColor="#FF4847"
    fontSize="16"
    fontWeight="normal"
    lineHeight="24"
    marginTop="16"
/>
<style id="Help--Error" color="#FF4847" />
<style id="MarginTop20" marginTop="20" />
<style
    id="H1"
    textAlign="center"
    color="#9da1ad"
    fontSize="24"
    fontWeight="bold"
    lineHeight="30"
    marginBottom="8"
/>
<style
    id="H2"
    textAlign="center"
    color="#9da1ad"
    fontWeight="bold"
    fontSize="20"
    lineHeight="22"
    marginBottom="8"
/>
<style
    id="H3"
    textAlign="center"
    color="#9da1ad"
    fontSize="14"
    lineHeight="20"
    marginBottom="8"
/>
<style
    id="Border"
    borderColor="black"
    borderWidth="1"
    borderRadius="10"
    margin="20"
    paddingVertical="10"
/>
<style
    id="BottomBorder"
    borderBottomColor="black"
    borderBottomWidth="1"
    margin="10"
    padding="20"
/>
<style
    id="List"
    marginBottom="10"
    padding="10"
/>
<style
    id="BtnActions"
    flexDirection="row"
    justifyContent="space-evenly"
    />
<style
    id="Primary"
    color="white"
    backgroundColor="#5c20dd"
    fontSize="16"
    fontWeight="bold"
    lineHeight="20"
    textAlign="center"
    width="100"
    padding="5"
    margin="5"
    borderRadius="10"
    borderWidth="1"
    borderColor="#5c20dd"
    overflow="hidden"
/>
<style
    id="DangerBtn"
    color="#b5605c"
    fontSize="16"
    fontWeight="bold"
    lineHeight="20"
    textAlign="center"
    width="100"
    padding="5"
    margin="5"
    borderRadius="10"
    borderWidth="2"
    borderColor="#b5605c"
    overflow="hidden"
/>
