<doc xmlns="https://hyperview.org/hyperview">
  <screen>
    <styles>
      <style id="Body" backgroundColor="white" flex="1" paddingTop="24" />
      <style
        id="Label"
        borderColor="#4E4D4D"
        fontSize="16"
        fontWeight="bold"
        lineHeight="24"
        marginBottom="8"
      />
        <style id="Wrapper"
        display="flex"
        justifyContent="center"
        flexDirection="row"
        flexWrap="wrap"
        width="100%"
        backgroundColor="white"
      />
        <style
        id="Profile"
        color="white"
        backgroundColor="#5c20dd"
        fontSize="16"
        fontWeight="bold"
        lineHeight="24"
        textAlign="center"
        width="100"
        padding="10"
        margin="10"
        borderRadius="10"
        borderWidth="1"
        borderColor="#bdc4c4"
        overflow="hidden"
      />
    </styles>
    <body style="Body" safe-area="true">
    <text style="Label">Your profile</text>
    <view style="Wrapper">
      <text
          action="close"
          href="{% url 'timary:mobile_home' %}"
          style="Profile"
        >
          Go back
        </text>
    </view>
    </body>
  </screen>
</doc>
